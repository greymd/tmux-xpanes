# Bash -- extract "MAJOR.MINOR" version. Ignore all the patch version's difference.
# Zsh -- extract "MAJOR.MINOR.PATCH" version from sourceforge https://sourceforge.net/projects/zsh/files/zsh/.
#         PATCH version is maximum or minimum ones within each MINOR version.
shell:bash,zsh,bash-3.0,bash-3.1,bash-3.2,bash-4.0,bash-4.1,bash-4.2,bash-4.3,bash-4.4,zsh-3.1.7,zsh-3.1.9,zsh-4.0.1,zsh-4.0.9,zsh-4.2.0,zsh-4.2.7,zsh-5.0.0,zsh-5.0.1,zsh-5.0.8,zsh-5.1,zsh-5.1.1,zsh-5.2,zsh-5.3,zsh-5.3.1
tmux:0.8,0.9,1.0,1.1,1.2,1.3,1.4,1.5,1.6,1.7,1.8,1.9,1.9a,2.0,2.1,2.2,2.3
os:linux(10),osx(1)

IF [os] = "osx" THEN [tmux] IN {"1.9","2.1","2.2","2.3"};
IF [tmux] IN {"0.8","0.9","1.0","1.1","1.2","1.3","1.4","1.5"} THEN [os] = "linux" AND [shell] = "bash";

